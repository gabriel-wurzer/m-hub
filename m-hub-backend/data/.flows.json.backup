    {
        "id": "5fb98f49e812cc42",
        "type": "function",
        "z": "bce2a8c06ab5a18b",
        "name": "build SQL query",
        "func": "// Extract the path parameter directly from the HTTP request\nlet id = msg.req.params.ID;\n\n// Validation\nif (id === undefined || id === null) {\n  msg.statusCode = 400;\n  msg.payload = { error: \"missing ID\" };\n  return msg;\n}\nid = String(id).trim();\n\n// Enforce varchar(10) semantics (length + allowed chars)\nif (id.length > 10 || !/^[A-Za-z0-9_\\-]+$/.test(id)) {\n  msg.statusCode = 400;\n  msg.payload = { error: \"invalid ID\" };\n  return msg;\n}\n\nmsg.query = 'SELECT * FROM buildings_details WHERE bw_geb_id = $1::varchar LIMIT 1;';\nmsg.params = [ id ];\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 260,
