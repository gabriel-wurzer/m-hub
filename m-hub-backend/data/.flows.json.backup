        "name": "format result",
        "func": "if (msg.payload && msg.payload.length > 0) {\n    msg.payload = msg.payload[0];\n} else {\n    msg.statusCode = 404;\n    msg.payload = { error: \"Building not found\" };\n}\nreturn msg;\n",
